<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zetta.fancier.getdata.dao.DataClassifyDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zetta.fancier.getdata.entity.DataClassify">
        <id column="id" property="id"/>
        <result column="class_name" property="className"/>
        <result column="parent_class_id" property="parentClassId"/>
        <result column="status" property="status"/>
        <result column="remark" property="remark"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, class_name AS className, parent_class_id AS parentClassId, status, remark
    </sql>

</mapper>
